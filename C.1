#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>
int factorial1(int n)
{
	int i = 0;
	int k = 1;
	for (i = 1; i <= n; i++)
	{
		k = k * i;
	}
	return k;
}

int factorial2(int n)
{
	if (n <= 1)
		return 1;
	else
		return n*factorial1(n-1);

}

int main()
{
	int n = 0;
	scanf("%d", &n);
	int sum = factorial2(n);
	printf("%d\n", sum);

}
